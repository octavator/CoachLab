{
  "name": "coachlab",
  "version": "1.0.0",
  "description": "React JS Client for CoachLab, ES6 compiled with babel",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "iex -S mix",
    "watch": "npx babel --watch web --presets @babel/preset-env --out-dir priv/static",
    "build": " mix webcopy && npx babel web --presets  @babel/preset-react --minified --out-dir priv/static && mix compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octavator/CoachLab.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/octavator/CoachLab/issues"
  },
  "homepage": "https://github.com/octavator/CoachLab#readme",
  "dependencies": {
    "@babel/preset-env": "^7.26.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/preset-env": "^7.12.1",
    "@babel/core": "^7.26.10",
    "@babel/preset-react": "^7.26.3"
  }
}
